<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--这个应该是底图的颜色-->
    <item>
        <shape>
            <corners android:radius="6dp"/>
            <solid android:color="#998752"/>
        </shape>
    </item>
    <!--这个应该是顶图的颜色-->
    <item
        android:bottom="1dp">
        <shape>
            <solid android:color="#FCEE12"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>
</layer-list>
